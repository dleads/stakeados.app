    "db:create-profile-for-existing-user": "node scripts/create-profile-for-existing-user.js",
    "db:create-admin-final": "node scripts/create-admin-final.js",
    "db:create-admin-with-uuid": "node scripts/create-admin-with-uuid.js",
    "db:verify-admin": "node scripts/verify-admin.js",
    "db:check-profiles": "node scripts/check-profiles.js",
    "db:list-admin-users": "node scripts/list-admin-users.js",
    "db:test-connection": "node scripts/test-connection.js",
    "db:verify-admin-credentials": "node scripts/verify-admin-credentials.js",
    "test-security": "node scripts/test-security.js",
    "debug-login": "node scripts/debug-login.js",
    "fix-admin-auth": "node scripts/fix-admin-auth.js",
    "check-status": "node scripts/check-system-status.js",
    "create-admin-manual": "node scripts/create-admin-manual.js",
    "test-login": "node scripts/test-login.js",
    "update-admin-password": "node scripts/update-admin-password.js",
    "reset-login-attempts": "node scripts/reset-login-attempts.js",
    "clear-login-state": "node scripts/clear-login-state.js",
    "check-admin-access": "node scripts/check-admin-access.js",
