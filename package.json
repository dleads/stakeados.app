    "test:seo": "playwright test e2e/seo.spec.ts",
    "test:performance": "playwright test e2e/performance.spec.ts",
    "test:lighthouse": "lhci autorun",
    "test:lighthouse:ci": "lhci autorun --upload.target=temporary-public-storage",
    "lighthouse:admin": "lighthouse http://localhost:3000/es/admin --output=html --output-path=./lighthouse-admin-report.html",
    "test:cross-browser": "vitest run src/test/cross-browser/",
    "test:a11y": "vitest run src/test/accessibility/",
    "test:quality": "npm run test:accessibility && npm run test:seo && npm run test:performance",
    "compile": "hardhat compile",
    "deploy:certificate:sepolia": "hardhat run scripts/deploy-certificate.js --network baseSepolia",
