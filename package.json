{
  "name": "stakeados-platform",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3000",
    "prebuild": "node scripts/deployment/clean-secrets.js",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "type-check:prod": "tsc -p tsconfig.prod.json --noEmit",
    "regenerate-types": "node scripts/regenerate-types.js",
    "type-check:app": "tsc -p tsconfig.json --noEmit",
    "type-check:tests": "tsc -p tsconfig.tests.json --noEmit",
    "analyze": "ANALYZE=true npm run build",
    "build:analyze": "npm run analyze",
    "prepare": "husky install",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest watch",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:jest": "jest",
    "test:jest:watch": "jest --watch",
    "test:jest:coverage": "jest --coverage",
    "test:all": "npm run test:run && npm run test:e2e",
    "test:admin-content": "npm run test:admin-unit && npm run test:admin-integration && npm run test:admin-e2e",
    "test:admin-unit": "vitest run test/admin-content-management/unit --config test/admin-content-management/vitest.config.ts",
    "test:admin-integration": "vitest run test/admin-content-management/integration --config test/admin-content-management/vitest.config.ts",
    "test:admin-e2e": "playwright test --config test/admin-content-management/e2e/playwright.config.ts",
